    "test:coverage": "nyc npm test",
    "dev": "nodemon"
  },
  "keywords": [
    "fretes",
    "EDI",
    "cargas",
    "gerenciamento"
  ],
  "author": "Luis <luis.rabock@hotmail.com>",
  "license": "MIT",
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "mocha": "^3.2.0",
    "morgan": "^1.9.0",
    "node-fetch": "^2.1.2",
    "nodemon": "^1.17.2",
    "nyc": "^10.2.0",
