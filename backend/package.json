    "express": "^4.16.3",
    "express-query-int": "^3.0.0",
    "fs": "0.0.1-security",
    "lodash": "^4.17.5",
    "moment": "^2.22.0",
    "mongoose": "^4.13.7",
    "mongoose-paginate": "^5.0.3",
    "mongoose-unique-validator": "^2.0.0",
    "multer": "^1.3.0",
    "pm2": "^2.10.1"
